<%- include("partials/header"); -%>
<h1>Home</h1>
<p><%= startingContent %></p>

<% if (posts.length > 0) { %> <% posts.forEach(function(post){ %>
<h1><%= post.title %></h1>
<% if (post.content) { %>
<p>
  <%= post.content.substring(0, 100) + " ..." %>
  <a href="/posts/<%= post._id %>">Read More</a>
</p>
<% } %> <% }) %> <% } else { %>
<p>No posts found.</p>
<% } %> <%- include("partials/footer"); -%>
